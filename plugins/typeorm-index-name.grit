engine biome(1.0)
language js(typescript)

`Index($...)` as $call where {
  not {
    $call <: `Index($name, $...)` where {
      $name <: or {
        `"$_"`,
        `'$_'`
      }
    }
  },
  register_diagnostic(span=$call, severity="error", message="@Index() must have an explicit name as the first argument, e.g. @Index('my_column_index')")
}
